{
   "containerDefinitions": [ 
      { 
         
         "essential": true,
         "image": "598780336447.dkr.ecr.us-east-2.amazonaws.com/test-site",
         "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/fargate-task-definition",
               "awslogs-region": "us-east-2",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "name": "test-site",
         "portMappings": [ 
            { 
               "containerPort": 5000,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::598780336447:role/ecsTaskExecutionRole",
   "family": "test-site",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}
